{
    "BUFFER_PROFILE": {
        "egress_lossless_profile": {
            "pool": "[BUFFER_POOL|egress_lossless_pool]",
            "size": "3584",
            "static_th": "224000"
        },
        "egress_lossy_profile_static": {
            "pool": "[BUFFER_POOL|egress_lossless_pool]",
            "size": "3584",
            "static_th": "112000"
        }
    },
    "BUFFER_PG": {
{% for port in minigraph_ports.keys() %}
        "{{ port }}|0": {
            "profile": "[BUFFER_PROFILE|ingress_lossy_profile]"
        }{{ "," if not loop.last else "" }}
{% endfor %}
    },
    "BUFFER_QUEUE": {
{% for port in minigraph_ports.keys() %}
        "{{ port }}|0-2": {
            "profile": "[BUFFER_PROFILE|egress_lossy_profile_static]"
        },
        "{{ port }}|3-4": {
            "profile": "[BUFFER_PROFILE|egress_lossless_profile]"
        },
        "{{ port }}|5-7": {
            "profile": "[BUFFER_PROFILE|egress_lossy_profile]"
        }{{ "," if not loop.last else "" }}
{% endfor %}
    },
    "QUEUE": {
{% for port in minigraph_ports.keys() %}
        "{{ port }}|0": {
            "scheduler": "[SCHEDULER|scheduler.2]",
            "wred_profile": "[WRED_PROFILE|AZURE_LOSSY]"
        },
        "{{ port }}|1-2": {
            "scheduler": "[SCHEDULER|scheduler.2]",
            "wred_profile": "[WRED_PROFILE|AZURE_LOSSY]"
        },
        "{{ port }}|3": {
            "scheduler": "[SCHEDULER|scheduler.1]",
            "wred_profile": "[WRED_PROFILE|AZURE_LOSSLESS]"
        },
        "{{ port }}|4": {
            "scheduler": "[SCHEDULER|scheduler.1]",
            "wred_profile": "[WRED_PROFILE|AZURE_LOSSLESS]"
        },
        "{{ port }}|5-7": {
            "scheduler": "[SCHEDULER|scheduler.2]",
            "wred_profile": "[WRED_PROFILE|AZURE_LOSSY]"
        }{{ "," if not loop.last else "" }}
{% endfor %}
    },
    "PFC_PRIORITY_TO_PRIORITY_GROUP_MAP": {
        "AZURE": {
            "3": "3",
            "4": "4"
        }
    },
    "PORT_QOS_MAP": {
{% for port in minigraph_ports.keys() %}
        "{{ port }}": {
            "pfc_to_pg_map": "[PFC_PRIORITY_TO_PRIORITY_GROUP_MAP|AZURE]",
            "pfc_to_queue_map": "[MAP_PFC_PRIORITY_TO_QUEUE|AZURE]",
            "pfc_enable"      : "3,4",
            "dscp_to_tc_map": "[DSCP_TO_TC_MAP|AZURE]",
            "tc_to_pg_map": "[TC_TO_PRIORITY_GROUP_MAP|AZURE]",
            "tc_to_queue_map": "[TC_TO_QUEUE_MAP|AZURE]"
